{
  "extends": [
    "wesbos/typescript",
    "react-app/jest"
  ],
  "rules": {
    "react/destructuring-assignment": 1,
    "react/prop-types": 0,
    "arrow-body-style": 0,
    "no-console": 0,
    "no-nested-ternary": 0,
    "no-plusplus": 0,
    "no-use-before-define": 0,
    "no-return-assign": 0,
    "@typescript-eslint/restrict-template-expressions": 0,
    "@typescript-eslint/naming-convention": 1,
    "@typescript-eslint/no-non-null-assertion": 0,
    "@typescript-eslint/no-use-before-define": [
      "error"
    ],
    "prettier/prettier": [
      1,
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
      }
    ]
  }
}