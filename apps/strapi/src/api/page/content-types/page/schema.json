{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "text_image_code",
        "text_image",
        "text_code",
        "video",
        "text",
        "blocks",
        "reflection"
      ],
      "required": true
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "media.text",
        "media.text-image",
        "media.text-image-code",
        "media.video",
        "media.text-code",
        "media.code-editor",
        "media.image"
      ],
      "required": true
    },
    "menu": {
      "type": "component",
      "repeatable": true,
      "component": "menu.menu-info"
    },
    "visible": {
      "type": "boolean",
      "default": true
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "media.link"
    },
    "concepts": {
      "type": "component",
      "repeatable": true,
      "component": "metadata.concept"
    }
